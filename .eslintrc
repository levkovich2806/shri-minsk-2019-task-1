{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": "error",
    "linebreak-style": 0,
    "indent": ["error", 2],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  },
  "extends": [
    "airbnb",
    "eslint:recommended"
  ],
  "settings": {
    
  }
}
